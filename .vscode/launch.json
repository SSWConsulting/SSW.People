{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Gatsby develop",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/.bin/gatsby",
        "autoAttachChildProcesses": true,
        "args": ["develop", "--inspect-brk"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      },
      {
        "name": "Gatsby build",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/.bin/gatsby",
        "args": ["build"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      }
    ]
  }